function monthToIndex(month) {
  var months = {
    'January': 1,
    'February': 2,
    'March': 3,
    'April': 4,
    'May': 5,
    'June': 6,
    'July': 7,
    'August': 8,
    'September': 9,
    'October': 10,
    'November': 11,
    'December': 12
  };
  return months[month];
}
//To use this function to convert the month name "October" into its corresponding index, you can call the function as follows:
var index = monthToIndex('October');
console.log(index); // Output: 10
